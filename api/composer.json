{
    "require": {
        "slim/slim": "4.14.0",
        "nyholm/psr7": "1.8.1",
        "nyholm/psr7-server": "1.1.0",
        "php-di/php-di": "7.0.6",
        "firebase/php-jwt": "6.10.1",
        "vlucas/phpdotenv": "5.6.0",
        "ramsey/uuid": "^4.7",
        "monolog/monolog": "^3.8",
        "illuminate/database": "^12.18",
        "illuminate/events": "^12.18",
        "illuminate/support": "^12.18",
        "illuminate/pagination": "^12.35",
        "spatie/data-transfer-object": "^3.9"
    },
    "autoload": {
        "psr-4": {
            "Base\\": "Base/",
            "Src\\": "src/",
            "Database\\": "Database/",
            "Middlewares\\": "Middlewares/",
            "Routes\\": "routes/"
        }
    },
    "config": {
        "process-timeout": 0
    },
    "scripts": {
        "start": "php -S localhost:1000 -t public/",
        "dump": "composer dump-autoload"
    }   
}
